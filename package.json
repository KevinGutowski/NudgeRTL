{
  "name": "nudgefromrtl",
  "version": "0.0.5",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "nudgefromRTL",
    "manifest": "src/manifest.json",
    "main": "nudgefromrtl.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KevinGutowski/NudgeRTL.git"
  },
  "description": "Quickly resize a layer horizontally from right to left with keyboard shortcuts",
  "author": "Kevin Gutowski\n <kskiviolin@gmail.com\n>"
}
